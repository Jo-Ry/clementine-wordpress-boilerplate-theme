{
	"name": "clementine-blocks",
	"version": "1.0.0",
	"description": "Enbales a set of blocks for the Clementine theme.",
	"author": "Clementine",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build": "wp-scripts build --blocks-manifest",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start --blocks-manifest",
		"create-block": "npx @wordpress/create-block@latest --namespace clementine-blocks --textdomain clementine --no-plugin"
	},
	"devDependencies": {
		"@wordpress/scripts": "^30.22.0"
	}
}
